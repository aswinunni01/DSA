#include <iostream>
using namespace std;
struct studentdat{
    char studentname[20];
    int rollno;
    int semester;
};
    

int main() {

    studentdat std1;
    cout << "Enter data for Student"<< endl;
    cin >> std1.studentname;
    cin >> std1.rollno;
    cin >> std1.semester;
    cout << "Display Information "<<endl;
    cout << "Student "<<std1.studentname <<" with roll number " << std1.rollno << " is studying in semester " << std1.semester;
    return 0;
}
